    },
    {
        "startTime": 19707,
        "endTime": 22819,
        "text": "This does not sound good, like it might hurt."
    },
    {
        "startTime": 23811,
        "endTime": 24579,
        "text": "Let's listen again."
    },
    {
        "startTime": 25734,
        "endTime": 27063,
        "text": "I couldn't believe that happened!"
    },
    {
        "startTime": 27489,
        "endTime": 27985,
        "text": "I know."
    },
    {
        "startTime": 28085,
        "endTime": 29456,
        "text": "Are you going to say something?"
    },
    {
