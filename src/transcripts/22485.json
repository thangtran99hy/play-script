[
    {
        "startTime": 3213,
        "endTime": 5723,
        "text": "What I am about to tell you sounds crazy,..."
    },
    {
        "startTime": 7719,
        "endTime": 9229,
        "text": "...but you have to listen to me."
    },
    {
        "startTime": 12225,
        "endTime": 14935,
        "text": "Your very lives depend on it."
    },
    {
        "startTime": 27105,
        "endTime": 29037,
        "text": "You see, this isn't the first time..."
    },
    {
        "startTime": 36352,
        "endTime": 37788,
        "text": "...that we've had this conversation."
    },
    {
        "startTime": 38887,
        "endTime": 39637,
        "text": "What day is it?"
    },
    {
        "startTime": 40173,
        "endTime": 41082,
        "text": "Judgment Day."
    },
    {
        "startTime": 41455,
        "endTime": 43467,
        "text": "You just came in with the fresh recruits."
    },
    {
        "startTime": 50187,
        "endTime": 53499,
        "text": "The invasion will fail along with every soldier you are sending."
    },
    {
        "startTime": 54212,
        "endTime": 56853,
        "text": "We lose everything."
    },
    {
        "startTime": 61240,
        "endTime": 63333,
        "text": "Come find me when you wake up!"
    },
    {
        "startTime": 75531,
        "endTime": 76931,
        "text": "You do know what's happening to me."
    },
    {
        "startTime": 77659,
        "endTime": 79301,
        "text": "What happened to you happened to me."
    },
    {
        "startTime": 82444,
        "endTime": 83865,
        "text": "You hijacked their power."
    },
    {
        "startTime": 86714,
        "endTime": 87454,
        "text": "I need your help."
    },
    {
        "startTime": 87852,
        "endTime": 88985,
        "text": "With what, exactly?"
    },
    {
        "startTime": 89151,
        "endTime": 89840,
        "text": "Winning the war."
    },
    {
        "startTime": 91299,
        "endTime": 92234,
        "text": "We can do this."
    },
    {
        "startTime": 92438,
        "endTime": 94587,
        "text": "Just come here every day and I'll train you."
    },
    {
        "startTime": 102203,
        "endTime": 104479,
        "text": "No matter what I do, this is as far as you go."
    },
    {
        "startTime": 106598,
        "endTime": 108313,
        "text": "Why does it matter what happens to me?"
    },
    {
        "startTime": 127567,
        "endTime": 129314,
        "text": "I'm not a soldier."
    },
    {
        "startTime": 132957,
        "endTime": 134851,
        "text": "Of course you're not; you're a weapon."
    }
]