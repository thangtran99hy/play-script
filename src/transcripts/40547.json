    },
    {
        "startTime": 22280,
        "endTime": 24753,
        "text": "I move you."
    },
    {
        "startTime": 26761,
        "endTime": 29791,
        "text": "I've captured your imagination since the beginning of time."
    },
    {
        "startTime": 31276,
        "endTime": 32349,
        "text": "You honor me."
    },
    {
        "startTime": 33147,
        "endTime": 34110,
        "text": "You chase me."
    },
    {
        "startTime": 35547,
        "endTime": 38459,
        "text": "I am a symbol of something wild that exists in you,..."
    },
    {
        "startTime": 40501,
        "endTime": 47657,
        "text": "...but lately it's I who feel wild,..."
    },
    {
        "startTime": 50165,
        "endTime": 55160,
        "text": "...choked by your trash, overheated by your pollution."
    },
    {
        "startTime": 55560,
