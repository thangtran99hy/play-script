[
    {
        "startTime": 4244,
        "endTime": 5695,
        "text": "Squirrel was scared."
    },
    {
        "startTime": 6350,
        "endTime": 7549,
        "text": "He could not eat."
    },
    {
        "startTime": 8081,
        "endTime": 9407,
        "text": "He could not sleep."
    },
    {
        "startTime": 9989,
        "endTime": 11687,
        "text": "He could only worry."
    },
    {
        "startTime": 13143,
        "endTime": 14922,
        "text": "\"What is the matter, Squirrel?\"..."
    },
    {
        "startTime": 15841,
        "endTime": 16859,
        "text": "...asked Raccoon."
    },
    {
        "startTime": 17759,
        "endTime": 19511,
        "text": "\"Why are you so worried?\""
    },
    {
        "startTime": 20747,
        "endTime": 22137,
        "text": "\"Haven't you noticed?\"..."
    },
    {
        "startTime": 22774,
        "endTime": 23779,
        "text": "...asked Squirrel."
    },
    {
        "startTime": 24663,
        "endTime": 26412,
        "text": "\"The moon is disappearing."
    },
    {
        "startTime": 26803,
        "endTime": 31852,
        "text": "We need to get all the food we can now, before it is too late.\""
    },
    {
        "startTime": 33367,
        "endTime": 36265,
        "text": "Deer and Raccoon looked up at the moon."
    },
    {
        "startTime": 37448,
        "endTime": 39515,
        "text": "\"The moon is not disappearing,\"..."
    },
    {
        "startTime": 40207,
        "endTime": 41241,
        "text": "...said Raccoon."
    },
    {
        "startTime": 42011,
        "endTime": 42973,
        "text": "\"Yes, it is,\"...."
    },
    {
        "startTime": 44369,
        "endTime": 45369,
        "text": "...said Squirrel."
    },
    {
        "startTime": 46573,
        "endTime": 49006,
        "text": "\"It is smaller than it was last night."
    },
    {
        "startTime": 49613,
        "endTime": 50349,
        "text": "Just wait."
    },
    {
        "startTime": 50760,
        "endTime": 53629,
        "text": "Tomorrow, it will be even smaller.\""
    },
    {
        "startTime": 54800,
        "endTime": 57902,
        "text": "The next night, the animals looked at the moon."
    },
    {
        "startTime": 59026,
        "endTime": 61585,
        "text": "\"Is the moon really getting smaller?\"..."
    },
    {
        "startTime": 62376,
        "endTime": 63338,
        "text": "...asked Deer."
    },
    {
        "startTime": 64487,
        "endTime": 65542,
        "text": "\"I don't know,\"..."
    },
    {
        "startTime": 66251,
        "endTime": 67239,
        "text": "...said Raccoon."
    },
    {
        "startTime": 68143,
        "endTime": 70873,
        "text": "But there was no question a few nights later."
    },
    {
        "startTime": 71583,
        "endTime": 73647,
        "text": "The moon really was smaller."
    },
    {
        "startTime": 75032,
        "endTime": 76492,
        "text": "\"Squirrel is right,\"..."
    },
    {
        "startTime": 77425,
        "endTime": 78373,
        "text": "...said Raccoon."
    },
    {
        "startTime": 79001,
        "endTime": 80805,
        "text": "\"The moon is disappearing.\""
    },
    {
        "startTime": 81988,
        "endTime": 85675,
        "text": "\"What is this I hear about the moon disappearing?\""
    },
    {
        "startTime": 86833,
        "endTime": 87796,
        "text": "...asked Skunk."
    },
    {
        "startTime": 89234,
        "endTime": 89768,
        "text": "\"Look!\""
    },
    {
        "startTime": 91062,
        "endTime": 93063,
        "text": "Squirrel pointed up at the moon."
    },
    {
        "startTime": 93785,
        "endTime": 96546,
        "text": "Only three-quarters of the moon was left now."
    },
    {
        "startTime": 97877,
        "endTime": 99541,
        "text": "\"It is disappearing,\"..."
    },
    {
        "startTime": 100443,
        "endTime": 101379,
        "text": "...said Skunk."
    }
]