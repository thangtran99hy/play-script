[
    {
        "startTime": 2643,
        "endTime": 6342,
        "text": "Wing went to Singapore to visit his Uncle Lee."
    },
    {
        "startTime": 7539,
        "endTime": 8997,
        "text": "Singapore was hot."
    },
    {
        "startTime": 10152,
        "endTime": 12543,
        "text": "Wing did not like to be hot."
    },
    {
        "startTime": 13713,
        "endTime": 16639,
        "text": "Uncle Lee liked to fish when it was hot."
    },
    {
        "startTime": 17782,
        "endTime": 20994,
        "text": "He took Wing to Bedok Jetty to fish."
    },
    {
        "startTime": 22049,
        "endTime": 23808,
        "text": "Wing had never fished."
    },
    {
        "startTime": 24913,
        "endTime": 28107,
        "text": "\"I don't like fishing,\" said Wing."
    },
    {
        "startTime": 29213,
        "endTime": 33459,
        "text": "\"How do you know if you have never tried?\" asked Uncle Lee."
    },
    {
        "startTime": 34576,
        "endTime": 37398,
        "text": "\"I just know,\" said Wing."
    },
    {
        "startTime": 38519,
        "endTime": 41559,
        "text": "\"Will you try?\" asked Uncle Lee."
    },
    {
        "startTime": 42748,
        "endTime": 45481,
        "text": "\"Can you teach me?\" asked Wing."
    },
    {
        "startTime": 46571,
        "endTime": 49245,
        "text": "\"Yes,\" said Uncle Lee."
    },
    {
        "startTime": 50375,
        "endTime": 52867,
        "text": "Uncle Lee gave Wing a fishing pole."
    },
    {
        "startTime": 53927,
        "endTime": 57139,
        "text": "He showed Wing how to put a worm on the hook."
    },
    {
        "startTime": 58293,
        "endTime": 61361,
        "text": "Uncle Lee threw his line into the water."
    },
    {
        "startTime": 62457,
        "endTime": 65488,
        "text": "Wing threw his line into the water, too."
    },
    {
        "startTime": 66703,
        "endTime": 69894,
        "text": "Uncle Lee felt something pull on his line."
    },
    {
        "startTime": 71027,
        "endTime": 73503,
        "text": "Wing looked to see what it was."
    },
    {
        "startTime": 74520,
        "endTime": 76643,
        "text": "Uncle Lee had caught a fish."
    },
    {
        "startTime": 77801,
        "endTime": 79867,
        "text": "Wing wanted to catch a fish."
    },
    {
        "startTime": 81029,
        "endTime": 83520,
        "text": "Uncle Lee had caught five fish."
    },
    {
        "startTime": 84577,
        "endTime": 86665,
        "text": "Wing had not caught any."
    },
    {
        "startTime": 87705,
        "endTime": 91116,
        "text": "\"I don't like fishing,\" said Wing."
    },
    {
        "startTime": 92244,
        "endTime": 95723,
        "text": "\"You will catch a fish,\" said Uncle Lee."
    },
    {
        "startTime": 96799,
        "endTime": 99567,
        "text": "Wing felt something pull on his line."
    },
    {
        "startTime": 100687,
        "endTime": 102883,
        "text": "He looked to see what it was."
    },
    {
        "startTime": 103939,
        "endTime": 105802,
        "text": "He did not see a fish."
    },
    {
        "startTime": 106894,
        "endTime": 109615,
        "text": "Wing pulled his line out of the water."
    },
    {
        "startTime": 110791,
        "endTime": 112419,
        "text": "The worm was gone!"
    },
    {
        "startTime": 113483,
        "endTime": 116586,
        "text": "Uncle Lee and Wing laughed together."
    },
    {
        "startTime": 117773,
        "endTime": 121147,
        "text": "\"I want to try again,\" said Wing."
    },
    {
        "startTime": 122236,
        "endTime": 125084,
        "text": "Uncle Lee put a worm on Wing's hook."
    },
    {
        "startTime": 126183,
        "endTime": 128827,
        "text": "Wing threw his line into the water."
    },
    {
        "startTime": 129915,
        "endTime": 132499,
        "text": "He felt something pull on his line."
    },
    {
        "startTime": 133621,
        "endTime": 136616,
        "text": "Uncle Lee looked to see what it was."
    },
    {
        "startTime": 137629,
        "endTime": 139469,
        "text": "Wing had caught a fish."
    },
    {
        "startTime": 140645,
        "endTime": 143157,
        "text": "Wing had caught a big fish."
    },
    {
        "startTime": 144284,
        "endTime": 148175,
        "text": "\"I like fishing in Singapore,\" said Wing."
    }
]